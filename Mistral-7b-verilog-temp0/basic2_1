// This is a module that implements an AND gate
module and_gate( 
input a, 
input b, 
output out );

// This is a module that implements an OR gate
module or_gate( 
input a, 
input b, 
output out );

// This is a module that implements an XOR gate
module xor_gate( 
input a, 
input b, 
output out );

// This is a module that implements an XNOR gate
module xnor_gate( 
input a, 
input b, 
output out );

// This is a module that implements a NAND gate
module nand_gate( 
input a, 
input b, 
output out );

// This is a module that implements a NOR gate
module nor_gate( 
input a, 
input b, 
output out );

// This is a module that implements a NOT gate
module not_gate( 
input a, 
output out );

